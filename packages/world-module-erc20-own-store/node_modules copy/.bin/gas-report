#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tamarar/Documents/Lattice/MUD repo/mud/packages/gas-report/dist/node_modules:/Users/tamarar/Documents/Lattice/MUD repo/mud/packages/gas-report/node_modules:/Users/tamarar/Documents/Lattice/MUD repo/mud/packages/node_modules:/Users/tamarar/Documents/Lattice/MUD repo/mud/node_modules:/Users/tamarar/Documents/Lattice/MUD repo/node_modules:/Users/tamarar/Documents/Lattice/node_modules:/Users/tamarar/Documents/node_modules:/Users/tamarar/node_modules:/Users/node_modules:/node_modules:/Users/tamarar/Documents/Lattice/MUD repo/mud/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tamarar/Documents/Lattice/MUD repo/mud/packages/gas-report/dist/node_modules:/Users/tamarar/Documents/Lattice/MUD repo/mud/packages/gas-report/node_modules:/Users/tamarar/Documents/Lattice/MUD repo/mud/packages/node_modules:/Users/tamarar/Documents/Lattice/MUD repo/mud/node_modules:/Users/tamarar/Documents/Lattice/MUD repo/node_modules:/Users/tamarar/Documents/Lattice/node_modules:/Users/tamarar/Documents/node_modules:/Users/tamarar/node_modules:/Users/node_modules:/node_modules:/Users/tamarar/Documents/Lattice/MUD repo/mud/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@latticexyz/gas-report/dist/gas-report.js" "$@"
else
  exec node  "$basedir/../@latticexyz/gas-report/dist/gas-report.js" "$@"
fi
